{
  "name": "Eurotrip",
  "version": "1.0.0",
  "description": "Eurotrip",
  "scripts": {
    "dev": "npm start",
    "start": "npm run clean && nodemon -x babel-node -- webpack/devserver.js ",
    "lint": "eslint src webpack --ignore-path .gitignore",
    "clean": "./scripts/clean_build.sh",
    "clean:cache": "CLEAN_CACHE_ONLY=1 ./scripts/clean_build.sh",
    "build:client": "node webpack/compiler.node.js",
    "build:static": "./scripts/build_static.sh",
    "build": "npm run clean && npm run build:client && npm run build:static",
    "build:test": "OUTPUT_FOLDER=builds/test NODE_ENV=staging    ./scripts/make_build.sh",
    "build:live": "OUTPUT_FOLDER=builds/live NODE_ENV=production ./scripts/make_build.sh",
    "deploy:test": "./scripts/make_deploy.sh test",
    "deploy:live": "./scripts/make_deploy.sh live",
    "translation:generate": "babel-node scripts/translation_generate.js",
    "translation:build": "babel-node scripts/translation_build.js",
    "dummy": "echo 'Trailing commas are nice.'"
  },
  "siteConfig": {
    "basename": "EuroTrip",
    "country": "en",
    "devPort": "8080"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.5",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "chokidar": "^1.4.3",
    "console-stamp": "^0.2.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^5.0.1",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.6.3",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.15.0",
    "image-webpack-loader": "^2.0.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.9.1",
    "react-helmet": "^3.0.2",
    "redbox-react": "^1.2.2",
    "style-loader": "^0.13.1",
    "toml": "^2.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.0",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "aphrodite": "^0.3.0",
    "babel-polyfill": "^6.7.4",
    "cookies-js": "^1.2.2",
    "express": "^4.14.0",
    "history": "^2.0.1",
    "intl": "1.1.0",
    "intl-messageformat": "^1.3.0",
    "isomorphic-fetch": "^2.2.1",
    "little-loader": "^0.1.1",
    "lodash": "^4.10.0",
    "mysql": "^2.12.0",
    "normalize.css": "^4.0.0",
    "react": " ^15.0 || ^15.0.0-0",
    "react-addons-css-transition-group": "^15.0.1",
    "react-dom": "^15.0 || ^15.0.0-0",
    "react-google-maps": "^6.0.1",
    "react-router": "^2.5.1",
    "react-select": "1.0.0 || 1.0.0-beta13",
    "scroll-behavior": "^0.4.0",
    "tinycolor2": "^1.3.0",
    "validate.js": "^0.9.0",
    "webfontloader": "^1.6.24"
  }
}
